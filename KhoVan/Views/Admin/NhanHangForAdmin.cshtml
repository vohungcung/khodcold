@{
    ViewBag.Title = "Cập nhật đầu 8";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<style>

    .colable {
        font-weight: bold;
    }

    .bt {
        width: 120px;
        height: 20px;
    }

    .tt {
        width: 30px
    }

    .sp {
        display: none;
    }

    .txtdate {
        width: 100px;
        height: 20px;
        text-indent: 10px;
    }

    .txtl {
        width: 250px;
        height: 20px;
        text-indent: 10px;
    }

    #Quantity {
        text-align: right;
    }
</style>
<script>
    $(window).load(function () {
       
    })
    
   
  
   
    function nhanhang() {
        $("#nhanhangbutton").hide();


        $.post('/admin/nhanhangforadmin', {
            VoucherID: $("#voucherid").val()
          
        }, function (result) {
            if (result.success) {
                alert(result.msg);
                location = location;

            } else {
                $.messager.show({    // show error message
                    title: 'Error',
                    msg: result.errorMsg
                });
                $("#nhanhangbutton").show();

            }
        }, 'json');
    }
   
</script>
<h1>
    Lấy hàng
</h1>


<table cellpadding="5" cellspacing="0">
    <tr>
        <td>Số phiếu nhận</td>
        <td>
            <input type="text" class="txtdate" id="voucherid" name="voucherid"  />
        </td>


    </tr>
   
    

    <tr>
        <td></td>
        <td>
            <input type="button" id="nhanhangbutton" name="name" value="Cập nhật" onclick="nhanhang()" />


        </td>
    </tr>
</table>
