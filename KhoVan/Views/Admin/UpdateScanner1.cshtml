@{
    ViewBag.Title = "Cập nhật scanner";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<style>

    .colable {
        font-weight: bold;
    }

    #txtcopy1, #txtcopy2 {
        width: 50px;
        height: 20px;
        text-align: right;
    }

    .bt {
        width: 120px;
        height: 20px;
    }

    .quycach {
        display: none;
    }

    .tt {
        width: 30px
    }

    .sp {
        display: none;
    }

    .txtdate {
        width: 100px;
        height: 20px;
        text-indent: 10px;
        text-align: right;
    }

    .txtl {
        width: 300px;
        height: 20px;
        text-indent: 10px;
    }

    .smallt {
        width: 100px;
    }

    .sl {
        text-align: right;
        width: 50px;
        height: 20px
    }

    .benphai {
        float: right;
        width: 700px;
    }

    .bentrai {
        float: left;
        clear: both;
    }

    #lastrow, #lastrow1 {
        background-color: navy;
        color: #FFF;
    }

        #lastrow a, #lastrow1 a {
            color: #FFF;
        }
</style>

<h1>
    Nhập scanner
</h1>

<table cellpadding="5" cellspacing="0" class="bentrai">

    <tr>
        <td>
            <span id="message"></span>
        </td>

        <td></td>
    </tr>
    <tr>
        <td> Số outbound </td>
        <td id="ob">
            @ViewBag.OB
        </td>
    </tr>
    @if (Global.Commons.ConvertToString(Request.QueryString["id"]) != "")
    {
        <tr>
            <td></td>
            <td>
                <input type="button" id="downloadfromsap" name="name" value="Lấy dữ liệu từ SAP" onclick="downloadfromsap(this)" />
            </td>
        </tr>
    }
    <tr>
        <td> Ngày kế hoạch xuất hàng  </td>
        <td>
            <input type="text" id="PlanDate" class="txtl" name="name" value="@ViewBag.PlanDate" />
        </td>
    </tr>
    <tr>
        <td> Mã khách  </td>
        <td>
            <input type="text" id="CustomerID" class="txtl" name="name" value="@ViewBag.CustomerID" />
        </td>
    </tr>
    <tr>
        <td> Tên khách  </td>
        <td>
            <input type="text" id="CustomerName" class="txtl" name="name" value="@ViewBag.CustomerName" />
        </td>
    </tr>
    <tr>
        <td>
            Địa chỉ
        </td>
        <td>
            <textarea id="Address" style="height:50px" class="txtl">@ViewBag.Address</textarea>
        </td>
    </tr>


    <tr>
        <td>
            Tổng số lượng
        </td>
        <td id="totalquantity">
            @ViewBag.TotalQuantity
        </td>
    </tr>
    <tr>
        <td>

            Số bao
        </td>
        <td id="bao">
            @ViewBag.Bag
        </td>
    </tr>
    <tr>
        <td>
            Số thùng
        </td>
        <td id="thung">
            @ViewBag.Box
        </td>
    </tr>
    <tr>
        <td>
            Số túi xốp
        </td>
        <td id="bxx">
            @ViewBag.BX
        </td>
    </tr>
    <tr>
        <td>
            Tổng số tiền
        </td>
        <td id="totalamount">
            @ViewBag.TotalAmount
        </td>
    </tr>
    <tr>
        <td>
            Tên user
        </td>
        <td>
            <input type="text" disabled="disabled" id="ScannerID" class="txtl" name="name" value="@ViewBag.ScannerID" />

        </td>
    </tr>
    <tr>
        <td>
            Nv quét

        </td>
        <td>
            <input type="text" id="EmployeeName" class="txtl" name="name" value="@ViewBag.EmployeeName" />

        </td>
    </tr>

    <tr>
        <td>
            Thể tích(m3)

        </td>
        <td id="m3">
            @ViewBag.M3

        </td>
    </tr>
    <tr>
        <td>
           Ghi chú

        </td>
        <td >
            <input type="text" id="note" maxlength="250" class="txtl" name="name" value="@ViewBag.Note" />

        </td>
    </tr>
</table>


