    function trove() {

        location = "/admin/ViewScanners?from=@DateTime.Now.ToString("dd/MM/yyyy")&to=@DateTime.Now.ToString("dd/MM/yyyy")";

    }
    function totalq() {
        var arr = $("#gthung").find(".row").get();

        var totalq1 = 0;
        var totalq2 = 0;
        var totalq3 = 0;

        for (var i = 0; i < arr.length; i++) {
            var r = $(arr[i]);
            var ll = $(r).find("input").get();
            var q1 =$( ll[0]).val();
            var q2 = $(ll[1]).val();
            var q3 = $(ll[2]).val();

            if (q1 == "")
                q1 = 0;
            if (isNaN(q1) == true)
                q1 = 0;

            if (q2 == "")
                q2 = 0;
            if (isNaN(q2) == true)
                q2 = 0;

            if (q3 == "")
                q3 = 0;
            if (isNaN(q3) == true)
                q3 = 0;

            if (parseInt(q1) > 0 && (parseInt(q2) > 0 || parseInt(q3) > 0)) {
                totalq1 += parseInt(q1);
                totalq2 += parseInt(q1) * parseInt(q2);

                totalq3 += parseInt(q3);


            }

        }
        $("#totalq1").html(totalq1);
        $("#totalq2").html(totalq2);
        $("#tx1").html(totalq3);
        $("#thung").html(totalq1);

        ttt();
    }
    function ttt() {
        var p1 = $.trim($("#totalq2").html());
        if (p1 == "")
            p1 = 0;
        var p2 = $.trim($("#totalq4").html());
        if (p2 == "")
            p2 = 0;
        var p3 = $.trim($("#tx1").html());
        if (p3 == "")
            p3 = 0;
        var p4 = $.trim($("#tx2").html());
        if (p4 == "")
            p4 = 0;

        var tt = parseInt(p1) + parseInt(p2) + parseInt(p3) + parseInt(p4);
        $("#tsl").html(tt);
        $("#tsltx").html(parseInt(p3) + parseInt(p4));

    }
    function totalq1() {
        var arr = $("#gbao").find(".row").get();
        var totalq1 = 0;
        var totalq2 = 0;
        var totalq3 = 0;

        for (var i = 0; i < arr.length; i++) {
            var r = $(arr[i]);
            var ll = $(r).find("input").get();
            var q1 = $(ll[0]).val();
            var q2 = $(ll[1]).val();
            var q3 = $(ll[2]).val();

            if (q1 == "")
                q1 = 0;
            if (isNaN(q1) == true)
                q1 = 0;

            if (q2 == "")
                q2 = 0;
            if (isNaN(q2) == true)
                q2 = 0;

            if (q3 == "")
                q3 = 0;
            if (isNaN(q3) == true)
                q3 = 0;

            if (parseInt(q1) > 0 && (parseInt(q2) > 0 || parseInt(q3) > 0)) {
                totalq1 += parseInt(q1);
                totalq2 += parseInt(q1) * parseInt(q2) ;
                totalq3 += parseInt(q3);



            }

        }
        $("#totalq3").html(totalq1);
        $("#totalq4").html(totalq2);
        $("#tx2").html(totalq3);

        $("#bao").html(totalq1);
        ttt();

    }
</script>

